{
    "functions": {
      "api/*.php": {
        "runtime": "vercel-php@0.7.1"
      }
    },
    "routes": [
      {
        "src": "/(.*)\\.mpd",
        "dest": "/api/mpd.php?id=$1&$query",
        "headers": {
          "Cache-Control": "max-age=20, public"
        }
      },
      {
        "src": "/(.*)\\.m3u8",
        "dest": "/api/index.php?id=$1",
        "headers": {
          "Cache-Control": "max-age=20, public"
        }
      },
      {
        "src": "/(.*)\\.key",
        "dest": "/api/keys.php?id=$1",
        "headers": {
          "Cache-Control": "max-age=20, public"
        }
      },
      {
        "src": "/",
        "dest": "/api/m3u.php",
        "headers": {
          "Cache-Control": "max-age=84000, public"
        }
      }
    ],
    "regions": ["bom1"]
  }